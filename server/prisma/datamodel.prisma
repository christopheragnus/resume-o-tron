type Post {
  id: ID! @id
  published: Boolean! @default(value: false)
  title: String!
  content: String!
}

type Resume {
  id: ID! @id
  name: String!
  address: String!
  email: String!
  positions: [Position!]!
}

type Position {
  id: ID! @id
  title: String!
  description: String!
  year: Int!
}
